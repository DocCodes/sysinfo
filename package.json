{
  "name": "sysinfo",
  "version": "0.1.11",
  "description": "Displays system information.",
  "keywords": [
    "desktop",
    "electron",
    "electron-app",
    "sys",
    "sysinfo",
    "system",
    "systeminformation"
  ],
  "homepage": "https://github.com/DocCodes/sysinfo#readme",
  "bugs": {
    "url": "https://github.com/DocCodes/sysinfo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DocCodes/sysinfo"
  },
  "license": "MIT",
  "author": "Evan Elias Young",
  "main": "main",
  "scripts": {
    "build-lux": "electron-packager . --overwrite --asar --platform=linux  --arch=all --prune=true --out=builds --app-copyright\"Copyright © 2018 Evan Elias Young\" --icon=\"assets/icons/png/1024x1024.png\"",
    "build-mac": "electron-packager . --overwrite --asar --platform=darwin --arch=all --prune=true --out=builds --app-copyright\"Copyright © 2018 Evan Elias Young\" --icon=\"assets/icons/mac/icon.icns\" --app-bundle-id=\"com.evanyoung.sysinfo\" --app-category-type=\"public.app-category.utilities\"",
    "build-win": "electron-packager . --overwrite --asar --platform=win32  --arch=all --prune=true --out=builds --app-copyright\"Copyright © 2018 Evan Elias Young\" --icon=\"assets/icons/win/icon.ico\"  --win32metadata.CompanyName=\"Evan Elias Young\" --win32metadata.FileDescription=\"Displays system information.\" --win32metadata.ProductName=\"SysInfo\"",
    "gh-release": "gh-release",
    "pre-release": "npm i --save-dev --save-prod && sort-package-json",
    "start": "electron .",
    "test": "standard && depcheck --ignores=standard,babel-eslint"
  },
  "dependencies": {
    "application-config": "^1.0.1",
    "bluebird": "^3.5.3",
    "bootstrap": "^4.1.3",
    "bootstrap-material-design": "^4.1.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.6",
    "stringify-object": "^3.3.0",
    "systeminformation": "^3.52.0"
  },
  "devDependencies": {
    "depcheck": "^0.6.11",
    "electron": "^3.0.11",
    "electron-packager": "^13.0.0",
    "electron-winstaller": "^2.7.0",
    "gh-release": "^3.4.0",
    "sort-package-json": "^1.17.0",
    "standard": "^12.0.1"
  }
}
